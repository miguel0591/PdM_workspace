#include <stdint.h>
#include <stdbool.h>

/* Private typedef -----------------------------------------------------------*/
typedef uint32_t tick_t; //
typedef bool bool_t;	  //

typedef struct {
   tick_t startTime;
   tick_t duration;
   bool_t running;
} delay_t;



delay_t retardo;                           //estructura del delay
tick_t duracion[] = {1000, 200, 100};      // diferentes duraciones en un array
