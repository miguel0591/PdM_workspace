#include "MEF.h"

static MEF_status MEF;

static LCD_printEstable(){
	LCD_2x16_cursor(1, 1);
	LCD_2x16_string("Estado:  Estable");
	LCD_2x16_cursor(2, 1);
	LCD_2x16_string("Temperatura:");
	LCD_2x16_cursor(2, 14);
	LCD_2x16_uint8(55);
}

void MEF_init(){
	MEF = ESTABLE;
}

void MEF_update(){
	switch(MEF){
		case ESTABLE:
			LCD_printEstable();
			//var temp = Sensor_tempRead();
			//if(temp){}

		break;

		case PRECAUCION:

		break;

		case PELIGRO:

		break;

	}
}
